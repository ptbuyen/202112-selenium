<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Regression" parallel="tests" thread-count="4">
    <test name="Buying Cheap Computer | Chrome" parallel="tests" thread-count="4">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="test.order.CheapComputerBuyingTest"/>
<!--            <class name="test.global.FooterTest" />-->
        </classes>
    </test>
    <test name="Buying Standard Computer | Firefox" parallel="tests" thread-count="4">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="test.order.StandardComputerBuyingTest"/>
        </classes>
    </test>
